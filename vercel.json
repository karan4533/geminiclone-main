{
  "functions": {
    "src/app/api/chat/route.ts": {
      "maxDuration": 60,
      "memory": 1024
    },
    "src/app/api/**/route.ts": {
      "maxDuration": 30
    }
  },
  "regions": ["iad1"],
  "env": {
    "GEMINI_API_KEY": "@gemini-api-key"
  },
  "rewrites": [
    {
      "source": "/api/chat",
      "destination": "/api/chat"
    }
  ]
}

